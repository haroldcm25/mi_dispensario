<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="mi_dispensario_proveedor_form_view" model="ir.ui.view">
		<field name="name">dispensario_proveedor_form</field>
		<field name="model">proveedor</field>
		<field name="arch" type="xml">
			<form string="Proveedor">
				<header>
<!--					<button name="funcion_mi_boton" string="Mi botÃ³n" type="object" class="btn btn-warning"/>-->
				</header>
				<sheet>
					<group>
						<field name="name"/>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record id="mi_dispensario_proveedor_tree_view" model="ir.ui.view">
		<field name="name">dispensario_proveedor_tree</field>
		<field name="model">proveedor</field>
		<field name="arch" type="xml">
			<tree string="Proveedores">
				<field name="name"/>
			</tree>
		</field>
	</record>

	<record id="action_proveedor_rvc" model="ir.actions.act_window">
		<field name="name">Proveedor</field>
		<field name="res_model">proveedor</field>
		<field name="type">ir.actions.act_window</field>
		<field name="view_mode">tree,form</field>
	</record>

	<menuitem
		id="hr_menu_proveedor_rvc"
		name="Proveedores"
		action="action_proveedor_rvc"
		parent="mi_dispensario.mi_dispensario_menu_principal"
		sequence="1"
		groups="mi_dispensario.group_mi_dispensario_user"/>

</odoo>